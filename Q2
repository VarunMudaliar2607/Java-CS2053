import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter a num");
        int num = sc.nextInt();
        int sum = 0;
        
        while (num>0){
            sum += num % 10;
            num/=10;
        }
        System.out.println("sum of digits "+sum);
    }
}
